{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "fomofactory",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": [
      "./src/**/*.ts"
    ]
  }
  